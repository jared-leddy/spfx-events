  "items": {
    "ONE_ITEM_SELECTED": {
      "title": { "default": "One Item Selected" },
      "iconImageUrl": "icons/request.png",
      "type": "command"
    },
    "TWO_ITEM_SELECTED": {
      "title": { "default": "Two Item Selected" },
      "iconImageUrl": "icons/request.png",
      "type": "command"
    },
    "ALWAYS_ON": {
      "title": { "default": "Always On" },
      "iconImageUrl": "icons/request.png",
      "type": "command"
    }
  }
